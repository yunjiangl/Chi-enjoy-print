<map id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="{{scale}}" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" circles="{{circles}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange"
  show-location style="width:100%; height: 340px;">


</map>
<!-- cover start  -->
<view class='cover' wx:if="{{flag}}">

  <!-- cover_01 start  -->
  <!-- <view class='cover_01'> -->
  <!-- <cover-image class="img" bindtap="change" src="{{showView?'../images/triangle_01.png':'../images/triangle_02.png'}}"></cover-image> -->
  <!-- showView start  -->
  <!-- <view class="{{showView?'hide':'show'}} coverList" > -->
  <!-- ||(valList.latitude>=(lon-0.0324)&&valList.longitude>=(lat-0.11038))  -->
  <view class='showAddress' wx:for="{{val}}" wx:key="" data-valList='{{valList}}' bindtap='getVal' wx:for-item="valList">
    <view wx:if="{{(valList.latitude<=lon&&valList.longitude<=lat) & valList.latitude>=lon-0.0324&&valList.longitude>=lat-0.11038}}">
    
      
      <view>{{valList.name}}</view>
      <view class='page'>
        <view class='black'>
          <view>黑白:{{blackmoney}}元/页</view>
        </view>
        <view>彩色:{{colormoney}}元/页</view>
      </view>
    </view>
    
  </view>
  <!-- </view> -->
  <!-- showView end -->
  <!-- </view> -->
  <!-- cover_01 end -->

</view>
<!-- cover end  -->