page{
  background: #f5f5f5;
}
.container{
  padding: 0px;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  justify-content: space-between;
  align-items: center;
  color: #333;
  font-size: 28rpx;
}
.top{
  color: #3798fb;
  margin: 5px 15px 8px 15px;
}
.content{
  background: #fff;
  border-top: 1px solid #e0e0e0;
  border-bottom: 1px solid #e0e0e0;
}
.list{
  display: flex;
  justify-content: space-between;
  padding: 12px 15px;
}
.left{
  display: flex;
}
.round{
  border: 1px solid #999;
  border-radius: 50%; 
  background: #fff;
  width: 15px;
  height: 15px;
  margin-right: 4px;
  margin-top: 2px;
}
.page{
  color: #666;
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin-left: 15px;
}