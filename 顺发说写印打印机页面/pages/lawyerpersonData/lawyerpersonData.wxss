page{
  background: #f5f5f5;
}
.container{
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  justify-content: space-between;
  align-items: center;
  color: #333;
  padding: 0px;
}
.top{
  background: #3798fb;
  width: 100%;
  display: flex;
  justify-content: space-between;
  line-height: 150px;
}
.left{
  display: flex;
  margin: 20px 0 0 20px; 
}
 .right{
  margin-right: 20px;
  color: #333;
} 
.name-vip{
  line-height: 28px;
  margin: 25px 0 0 20px;
  color: #fff;
}
.vip{
  font-size: 12px;
}
.header-photo image{
  width: 50px;
  height: 50px;
}
.right image{
  width: 20px;
  height: 19px;
}
.center{
  background: #fff;
  width: 100%;
  height: 100%;
  margin-top: 20px;
  border: 1px solid #e0e0e0;
  border-right: none;
  border-left: none;
}
.list-until{
  display: flex;
  color: #666;
  justify-content: space-between;
  line-height: 44px;
  margin: 0 20px;
}
.question image{
  width: 15px;
  margin: 15px 10px 0 0;
  height: 15px;
}
.question{
  display: flex;
}
.go{
  font-size: 18px;
  font-family: '楷体';
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin: 0px 0 0 20px;
}
.footer{
  margin:  50px 20px 50px 20px;
}
button[type="default"] {
color:#fff;
background-color:#3798fb;
}
.view_hide{  
 display: none;  
}  
.view_show{  
 display:flex;  
} 