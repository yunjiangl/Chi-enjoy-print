page{
  background: #f5f5f5;
}
.container{
  padding: 0;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  color: #333;
  height: 100%;
}
.content{
  background: #fff;
  width: 100%;
  color: #333;
  font-size: 14px;
  padding: 20px 0px 20px 20px;
  border-bottom: 1px solid #e0e0e0;
}
.word{
  width: 85%;
  display: flex;
  margin-top: 20px;
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin-top: 20px;
}
.WordAndDate{
  flex-direction: row;
  margin-left: 15px;
  margin-top: 5px;
  width: 80%;
  line-height: 25px;
}
.wordpng{
  width: 64px;
  height: 76px;
}
.PageDate{
  font-size: 14px;
  color: #999;
}
.pdf{
  width: 64px;
  height: 65px;
}
.excel{
  width: 58px;
  height: 66px;
}