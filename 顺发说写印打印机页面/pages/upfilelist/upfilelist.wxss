page{
  background: #f5f5f5;
}
.container{
  padding: 0;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  color: #333;
  height: 100%;
}
.content{
  background: #fff;
  width: 100%;
  color: #333;
  padding: 20rpx 0rpx 40rpx 0rpx; 
  border-bottom: 1px solid #e0e0e0;
}
.word{
  width: 85%;
  display: flex;
  margin: 40rpx 0 0 40rpx;
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin: 30rpx 0 0 40rpx;
}
.WordAndDate{
  flex-direction: row;
  margin-left: 30rpx; 
  margin-top: 10rpx;
  width: 80%;
  line-height: 30rpx;
}
.wordpng{
  width: 128rpx;
  height: 152rpx;
}
.PageDate{
  color: #999;
  margin-top: 20rpx;
}
.pdf{
  width: 128rpx;
  height: 130rpx;
}
.excel{
  width: 116rpx;
  height: 132rpx;
}