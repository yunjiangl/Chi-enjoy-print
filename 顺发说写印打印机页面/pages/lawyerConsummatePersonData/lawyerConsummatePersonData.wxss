/* 下拉框 */
.phone_personal{
    width: 100%;
    height:100rpx;
    line-height:100rpx;
    color: #999;
}
.phone_one{
    display: flex; 
    position: relative;
    justify-content: space-between;
    background: #fff;
    width: 520rpx;
    height:100rpx;
    margin:0 auto;
    line-height: 400rpx;
    z-index: 1;
}
.person_box{
    position: relative;
    z-index: 10;
}
.phone_select{
    margin-top:0;
    margin-left: -15px;
    z-index: 1;
    position: absolute; 
}
.select_one{
    text-align: center;
    background: #fff;
    width:538rpx;   
    height:100rpx;
    line-height:90rpx;
    margin:0 5%;
    border-bottom:1rpx solid rgb(255, 255, 255);
}
.personal_image{
    z-index: 10;
    position: absolute;
    transition: All 0.4s ease;  
    -webkit-transition: All 0.4s ease;
}
.rotateRight{
    transform: rotate(180deg); 
}
.occupationMechanism,.occupationNumber{
  display: flex;
  line-height: 100rpx;
  z-index: 333;
}
.font{
  margin-right: 30rpx;
  padding-left: 30rpx;
}
.infoOccupation{
  margin-top: 26rpx;
  z-index: 2;
}
.atLeast{
  color: #999;
}
.choice{
  width: 100%;
  background: #fff;
  overflow: hidden;
  padding-bottom: 10rpx;
}
.rt{
  width: 100%;
  margin-left: 5px;
  margin-right: 5px;
}
.list{
  border: 1px solid #e0e0e0;
  border-radius: 20px;
  line-height: 25px;
  padding: 0 10px;
  margin: 10rpx 15rpx;
  float: left;
}
 .rt .active{
  color: #ff6700;
  border: 1px solid #ff6700;  
} 
.active-tag {
  color:#ff6700;  
  border: 1px solid #ff6700;
  border-radius: 20px;
  line-height: 25px;
  padding: 0 10px;
}
