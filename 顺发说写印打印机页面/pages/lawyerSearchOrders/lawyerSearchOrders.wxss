page{
  background: #f5f5f5;
}
.container{
  padding: 0px;
  width: 100%;
  height: 100%;
  align-items: center;
  box-sizing: border-box;
  justify-content: center;
  color: #333;
}
.top{
  background: #fff;
  display: flex;
  justify-content: space-between;
  padding-right: 20px;
  padding-left: 20px;
  border-bottom: 1px solid #e0e0e0;
  background: #fff;
}
.right{
  line-height: 60px;
}
.search image{
  width: 15px;
  height: 15px;
  margin: 8px 5px 0 15px;
}
.search{
  overflow: hidden;
  margin: 13px 0 10px 5px; 
  border-radius: 20px;
  border: none;
  width: 80%;
  font-size: 12px;
  text-align: center;
  background: #f5f5f5;
  height: 30px;
  display: flex;
}
.input{
  margin: 3px 0 8px 5px;
  width: 75%;
  text-align: left;
}
.dateAndMoney{
  display: flex;
  justify-content: space-between;
  line-height: 25px;
  background: #f5f5f5;
  padding: 10px 15px;
  margin-bottom: -20px;
}
.income{
  color: #999;
  margin-right: 20px;
}
.warn{
  color: #f00;
  font-size: 12px;
}
.date{
  margin-top: 10px;
}
.date image{
  width: 25px;
  height: 26px;
}
.list{
  background: #fff;
  border: 1px solid #e0e0e0;
  border-left: none;
  border-right: none;
  padding: 15px 0;
  margin-top: 20px;
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin: 13px 0;
  width: 100%;
}
.owner{
  display: flex;
  justify-content: space-between;
  padding: 0 15px;
  color: #666;
}

.customer{
  display: flex;
  justify-content: space-between;
  padding: 0 15px;
}
.yearDate{
  color: #999;
  font-size: 12px;
  margin-top: 5px;
}
.customerMoner{
  margin-top: 10px;
}