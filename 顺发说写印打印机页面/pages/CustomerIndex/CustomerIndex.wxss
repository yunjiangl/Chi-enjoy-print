page{
  background: #f5f5f5;
}
.container {
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding:0px 0;
  color: #333;
  background: #f5f5f5;
} 
.slide-image{
  width: 100%;
  height: 150px;
}
.content{
  height: 634px;
  display: flex; 
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding:0px 10px;
  background: #f5f5f5;
}
.main_01{
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 240rpx;
  margin-top: 22rpx;
}
.main_01_left,.main_01_right{
  width: 45%;
  background: #fff;
  padding: 20rpx;
  flex-direction: row;
  display: flex;
  font-size: 14px;

  
  justify-content: center;
  color: #999;
}
.photo-Print{
  margin-top: 20px;
  width: 50%;
}
.photo-image{
  line-height: 120px;
}
.print{
  display: block;
  font-weight: bold;
  font-size: 14px;
  color: #333;
}
.main_01_right{
  margin: 0 0px 0 5px;
}
.main_01_left{
  margin: 0 5px 0 0px;
}
.main_01_left image{
  width: 36px;
  height: 31px;
  margin-left: 10px;
}
.main_01_right image{
  width: 36px;
  height: 45px;
  margin-left: 15px;
}
/* main_02  */
.main_02{
  margin: 20px 10px 20px 10px;
  display: flex; 
  width: 100%;
  font-size: 14px;
  color: #333;
  height: 120px;
  background: #fff;
}
.image_02{
  line-height: 150px;
}
.text_02{
  width: 70%;
  margin: 25px 17px 20px 20px;
}
.main_02 image{
  width: 50px;
  height: 50px;
}
.text_03{
  line-height: 120px;
  width: 67%;
  margin: 0px 30px 20px 20px;
}
.image_03{
  line-height: 150px;
}
.text_04{
  width: 65%;
  margin: 35px 35px 20px 20px;
}
.footer{
  width: 100%;
  display: flex;
  margin-bottom: 20px;
  justify-content:center;
}
.footer image{
  width: 20px;
  padding-top: 10px;
  margin-right: 8px;
  height: 20px;
}
.btn-left{
  margin-right: 20rpx;
}
.btn-right{
  margin-left: 20rpx;
}

button[size='mini']{
  display: flex;
  font-size: 14px;
  width: 100%;
  line-height: 40px;
}
button[type='default']{
  color: #fff;
  background: #3798fb;
}