page{
  background: #fff;
}
.container{
  padding: 0px;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  height: 100%;
  color: #333;
  font-size: 14px;
  margin: 20px 0 20px 20px;
}
.list{
  display: flex;
  background: #fff;
  line-height: 50px;
  justify-content: space-between;
  padding-right: 20px;
}
.center{
  line-height: 30px;
}
.left{
  display: flex;
}
.left image{
  width: 40px;
  height: 40px;
  margin-right: 15px;
}
.details{
  display: flex;
}
.lawyer-details{
  color: #666;
  margin: 0 5px;
  font-size: 12px;
  border: 1px solid #e0e0e0;
  border-radius: 10px;
  line-height: 20px;
  padding: 0 5px;
}
.right{
  font-size: 20px;
  font-family: "楷体";
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin: 15px 0px;
}