page{
  background: #f5f5f5;
}
.container{
  padding: 0px;
  width: 100%;
  height: 100%;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  color: #333;
  font-size: 14px;
}
.top{
  display: flex;
  width: 100%;
  background: #fff;
  padding: 20px 0 10px 20px; 
  border-bottom: 1px solid #e0e0e0;
}
.image image{
  width: 100px;
  height: 104px;
}
.unitl{
  display: flex;
  margin-left: 10px;
}
.top-information{
  line-height: 35px;
}
.lt{
  margin-right: 5px;
}
.center{
  background: #fff;
  width: 100%;
  margin-top: 20px;
  padding-left: 20px;
  border: 1px solid #e0e0e0;
  border-left: none;
  border-right: none;
}
.center-number{
  line-height: 50px;
  display: flex;
}
.line{
  height: 1px;
  background: #e0e0e0;
}
.footer{
  width: 100%;
  background: #fff;
  margin-top: 20px;
  padding-left: 20px;
  border: 1px solid #e0e0e0;
  border-left: none;
  border-right: none;
  padding-bottom: 10px;
}
.footer-font{
  line-height: 50px;
  display: flex;
}
.list{
  border: 1px solid #e0e0e0;
  border-radius: 20px;
  line-height: 25px;
  padding: 0 10px;
  margin-top: 15px;
  margin-left: 5px;
}
.rt{
  margin-left: 5px;
}
.tp{
  display: flex;
}
.btn{
  margin:  30px 20px 10px 20px;
}
button[type="default"]{
  color: #fff;
  background: #3798fb;
}