page{
  background: #f5f5f5;
}
.container {
  height: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding:0px 0;
  color: #333;
  background: #f5f5f5;
}
.top{
  display: flex;
  justify-content: flex-start;
  padding: 30rpx 0 30rpx 30rpx;
  border-bottom: 1px solid #e0e0e0;
  background: #fff;
}
.top image{
  width: 60rpx;
  height: 60rpx;
  margin-right: 30rpx;
  margin-top: 14rpx;
}
.top-right{
  line-height: 42rpx;
  font-size: 28rpx;
}
.content{
  margin-top: 40rpx;
  padding: 30rpx 0; 
  border-top: 1px solid #e0e0e0;
  border-bottom: 1px solid #e0e0e0;
  background: #fff;
}
.title{
  margin-left: 30rpx;
  margin-bottom: 30rpx;
  font-weight: bold;
  font-size: 28rpx;
}
.ul{
  padding-top: 30rpx;
  padding-left: 30rpx; 
   border-top: 1px solid #e0e0e0; 
}
.ul image{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  border-radius: 10px; 
  border: 1px solid #e0e0e0;
}
.li{
  display: flex;
  /* padding-bottom: 30rpx;
  margin-bottom: 30rpx; */
}
.ul-right{
  line-height: 50rpx;
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin: 30rpx  0;
}
.li-title{
  font-size: 28rpx;
}
.explain{
  font-size: 24rpx;
  color: #676767;
}