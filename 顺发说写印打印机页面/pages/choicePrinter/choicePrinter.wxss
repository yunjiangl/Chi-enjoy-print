page{
  background: #f5f5f5;
}
.container {
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding:0;
  color: #333;
  height: 100%;
}
.top{
  display: flex;
  width: 100%;
  color: #666;
  justify-content:center;
}
.address image{
  width: 13px;
  height: 17px;
  margin:5px 8px 0 15px;
}
.address{
  margin-bottom: 20px;
  margin-top: 20px;
  margin-right: 5px;
  border-radius: 20px;
  background: #fff;
  border: none;
  line-height: 30px;
  display: flex;
  width: 48%;
  font-size: 12px;
  text-align: center;
}
.search image{
  width: 15px;
  height: 15px;
  margin: 8px 5px 0 15px;
}
.search{
  overflow: hidden;
  margin: 20px 0 20px 5px; 
  border-radius: 20px;
  border: none;
  width: 48%;
  font-size: 12px;
  line-height: 30px;
  text-align: center;
  background: #fff;
  display: flex;
}
.input{
  margin: 3px 0 0 5px;
  width: 70%;
  text-align: left;
}
.list{
  width: 100%;
  background: #fff;
  border-bottom: 1px solid #e0e0e0;
  color: #333;
}
.list-unit{
  line-height: 50px;
  padding: 0 10px;
}
.lable{
  font-size: 14px;
  float:right;
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin-left: 10px;
}