page{
  background: #f5f5f5;
  font-size: 28rpx;
}
.container{
  padding: 0px;
  width: 100%;
  height: 100%;
  align-items: center;
  box-sizing: border-box;
  justify-content: center;
  color: #333;
}
.top{
  background: #fff;
  line-height: 60px;
  display: flex;
  justify-content: space-between;
  padding-right: 20px;
  padding-left: 20px;
  border-bottom: 1px solid #e0e0e0;
}
.search image{
  width: 15px;
  height: 15px;
  margin: 8px 5px 0 15px;
}
.search{
  overflow: hidden;
  margin: 15px 0 20rpx 5px; 
  border-radius: 20px;
  border: none;
  width: 100%;
  font-size: 12px;
  line-height: 50px;
  text-align: center;
  background: #f5f5f5;
  display: flex;
}
.input{
  margin: 5px 0 8px 5px;
  width: 75%;
  text-align: left;
}
.content{
  margin-top: 40rpx;
  background: #fff;
  border-top: 1px solid #e0e0e0;
  border-bottom: 1px solid #e0e0e0;  
}
.device-owner{
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #e0e0e0;
  padding: 30rpx;
  background: #fff;
}
.device-owner label{
  margin-right: 10rpx;
}
.li{
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0 30rpx 0rpx;
  margin-left: 30rpx;
}
.li label{
  margin-right: 10rpx;
  color: #555;
}
.address{
  margin-bottom: 10rpx;
}
.line{
  height: 1px;
  background: #e0e0e0;
  margin-left: 30rpx;
}
.btn{
  display: flex;
  justify-content: space-between;
  margin-right: 10px;
}
.btn button{
  margin: 0 10rpx 5px 18rpx;
}
 button[type='primary']{
  color: #fff;
  background: #3798fb;
} 
button[type='default']{
  color: #3798fb;
  border: 1px solid #3798fb;
}
.button-hover{
  background: #3798fb;
}
.button::after{
  border: none;
}
.footer{
  display: flex;
  margin: 100rpx 0 40rpx 0; 
}
.footer button{
  width: 90%;
}
.costs{
  display: flex;
  justify-content: space-between;
}
.footer button[type='default']{
  color: #fff;
  background: #3798fb;
}