<!--container start  -->
<view class='container'>

<!-- content start  -->
<view class='content'>

  <!--top start  -->
  <view class='top'>
      <view class='address'>
         <image src='../images/address.png' />
         <label>贵州遵义市红花岗区</label>
      </view>
        <view class='search'>
        <image src='../images/search.png' />
          <input class='input' confirm-type="search" placeholder='输入物主名或设备编号'> </input>
        </view>
  </view>
  <!-- top end  -->

  <!-- list start  -->
  <view class='list'>

    <view wx:for='{{details}}' wx:for-index="idx" wx:for-item="ins">
    <view class='list-unit' bindtap='managerOn' data-key='{{ins[idx].createId}}'>
      <label class='label'>{{ins[idx].managerName}}</label>
      <label class='lable'>{{ins[idx].province}}{{ins[idx].city}}{{ins[idx].area}} <tt></tt> ></label>
    </view>

    <view class='line' wx:if='{{(idx+1)!=details.length}}'></view>
    </view>
    
  </view>
  <!--list end  -->

</view>
<!--content end  -->
</view>
<!-- container end  -->