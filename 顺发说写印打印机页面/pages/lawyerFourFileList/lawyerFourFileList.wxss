page{
  background: #f5f5f5;
}
.container{
  color: #333;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  height: 100%;
  width: 100%;
  padding: 0px;
}
.content{
  background: #fff;
  border-bottom: 2rpx solid #e0e0e0;
}
.list{
  line-height: 88rpx;
  margin: 0 15px;
  display: flex;
  justify-content: space-between;
}
.line{
  height: 2rpx;
  margin-left: 15px;
  background: #e0e0e0;

}