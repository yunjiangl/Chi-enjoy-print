page{
  background: #f5f5f5;
}
.container{
  padding: 0px;
  width: 100%;
  height: 100%;
  align-items: center;
  box-sizing: border-box;
  justify-content: center;
  color: #333;
}
.top{
  background: #fff;
  line-height: 60px;
  display: flex;
  justify-content: space-between;
  padding-right: 20px;
  padding-left: 20px;
  border-bottom: 1px solid #e0e0e0;
}
.right{
  line-height: 60px;
}
.search image{
  width: 15px;
  height: 15px;
  margin: 8px 5px 0 15px;
}
.search{
  overflow: hidden;
  margin: 15px 0 20px 5px; 
  border-radius: 20px;
  border: none;
  width: 80%;
  font-size: 12px;
  line-height: 50px;
  text-align: center;
  background: #f5f5f5;
  display: flex;
}
.input{
  margin: 5px 0 8px 5px;
  width: 75%;
  text-align: left;
}